      "source": [
        "import numpy as np\n",
        "#criando um data frame\n",
        "df_salarios= pd.DataFrame ({\n",
        "   'Nome': ['Bruna', 'Rafael', 'Carlos','Daniela', 'Valquiria'],\n",
        "    'Salario': [5500, np.nan, 4000, np.nan, 100000]\n",
        " })\n",
        "#substitui os valores nulos e substitui pela média e arredonda em até 2 casas\n",
        "df_salarios['salario_media'] = df_salarios['Salario'].fillna(df_salarios['Salario'].mean (). round(2))\n",
        "#mediana deixa os valores mais prox da realidade, ja que um dos numeros pode distorcer o valor médio acima\n",
        "df_salarios['salario_mediana'] = df_salarios['Salario'].fillna(df_salarios['Salario'].median ())\n",
        "df_salarios"
